{
  "name": "gitmind-ai-repo-analyst",
  "description": "AI-powered GitHub repository analyzer using Google Gemini",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:dev": "PLAYWRIGHT_BASE_URL=${PLAYWRIGHT_BASE_URL:-http://localhost:5173} playwright test",
    "test:uat": "PLAYWRIGHT_BASE_URL=${PLAYWRIGHT_BASE_URL:-http://localhost:4173} playwright test",
    "uat": "npm run build && concurrently -k -s first -n preview,test \"npm run preview -- --port 4173\" \"wait-on http://localhost:4173 && npm run test:uat\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@supabase/supabase-js": "^2.95.3",
    "lucide-react": "0.460.0",
    "mermaid": "^10.9.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reactflow": "11.11.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "typescript": "~5.6.3",
    "vite": "^6.2.0",
    "wait-on": "^7.2.0"
  }
}
